<div style="border: 1px solid black; border-radius: 20px; padding: 20px; width: 250px">
  <div><img src="<%= player.photo %>" style="height: 200px; object-fit: cover" /></div>
  <div>
    <p>Name: <%= player.name %> <%= player.lastName %></p>
    <div>
      <% player.average.forEach((yearAverage) => {%>
      <div class="col-md-6">
        <p><%= yearAverage.year %></p>
      </div>
      <div class="col-md-6">
        <p><%= yearAverage.points %></p>
      </div>
      <% }) %>
    </div>
  </div>
</div>
